{
  "name": "queetings",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build-peer": "node build.js browser src/peer.html peer/index.html",
    "build-beacon": "node build.js node src/beacon.ts beacon/main.js",
    "build": "npm run build-peer; npm run build-beacon",
    "clean": "rm -rf peer beacon .cache node_modules; npm install",
    "rebuild": "npm run clean; npm run build",
    "deploy": "npm run rebuild; gcloud app deploy",
    "start": "node beacon/main.js",
    "stop": "ps x | grep 'node beacon/main.js' | awk '{system(\"kill \"$1)}'",
    "dev": "node dev.js",
    "test": "exit 0"
  },
  "author": "abetaev",
  "license": "ISC",
  "dependencies": {
    "node-static": "^0.7.11",
    "stun": "^2.1.0",
    "uuid": "^3.4.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "@types/node-static": "^0.7.3",
    "@types/uuid": "^3.4.9",
    "@types/ws": "^7.2.4",
    "clipboard-polyfill": "^2.8.6",
    "jest": "^25.5.4",
    "node-watch": "^0.6.4",
    "parcel-bundler": "^1.12.4",
    "preact": "^10.4.1",
    "preact-material-components": "^1.6.1",
    "typescript": "^3.8.3",
    "webrtc-adapter": "^7.6.0"
  }
}
